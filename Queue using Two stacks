int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int n,q,ele;
    queue<int>que;
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&q);
        if(q==1)
        {
            scanf("%d",&ele);
            que.push(ele);
        }
        else if(q==2)
        {
            que.pop();
        }
        else if(q==3)
        {
            cout<<que.front()<<endl;
        }
        else {
        
        }
    }
    return 0;
}
